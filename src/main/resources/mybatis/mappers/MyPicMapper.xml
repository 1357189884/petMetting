<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.mapper.MyPicMapper">
	
	<insert id="insertImgUrl" parameterType="cn.tedu.pojo.PetShow">
		INSERT INTO pet_show(id,user_id,NAME,create_time,imgurl,description,times)
		VALUES(#{id},#{userId},#{name},#{createTime},#{imgurl},#{description},#{times});
	</insert>
	
</mapper>
